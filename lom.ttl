                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   lom2owl:educationalInteractivityLevel
                         ] ;
        terms:type       ""^^rdfs:Literal .

